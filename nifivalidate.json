      },
      "address": {
        "type": "string"
      },
      "created_at": {
        "type": "string",
        "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}$"
      }
    },
    "required": ["first_name", "last_name", "email", "phone_number", "address", "created_at"]
}
